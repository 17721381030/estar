html, body, div, span, a,font, img, form, table, tbody,tr, th, td {padding: 0px;margin: 0px;border: 0px;outline: 0px;}

html {
	_width: 100%;
	_height: 100%;
	-webkit-text-size-adjust: none;
	-ms-text-size-adjust: none;
	_filter: expression(document.execCommand("BackgroundImageCache", false, true));
}

body {
	_width: 100%;
	_height: 100%;
	min-width: 1000px;
	font: 13px Microsoft Yahei;
	_background-attachment: fixed;
	background: #ffffff;
}
.sys_name{
	font: 28px Microsoft Yahei;
	padding-left:10px;
}
div.top{
	width:100%;
	height:305px; 
	background:url(../images/bg.png) repeat-x; 
	float:left;
}
div.top span{ 
	width:330px; 
	height:140px; 
	margin:0 auto; 
	margin-top:80px; 
	display:block;
}
div.login {
	width: 410px;
	height: auto;
	margin:0 auto;
	margin-top:8px;
	overflow: hidden;
	margin-left:39%;
	float:left;
}

div.login .mainTable {
	margin:10px;
	padding:2px;
}

div.login th {
	height: 25px;
	padding-right: 6px;
	font-weight: normal;
	text-align: right;
}


div.login .powered {
	height: 30px;
	line-height: 30px;
	padding-right: 10px;
	text-align: center;
	font-size: 12px;
	color: #999999;
	margin-top:30px;
}
a {
	color: #666666;
	text-decoration: none;
}
div.login .link {
	height: 30px;
	line-height: 30px;
	padding-right: 7px;
	text-align: center;
	color: #888888;
}

div.login .link a {
	margin: 0px 3px;
}

table.fields td{
	padding:0 20px 0 20px;
	width: 300px;
	height: 25px;
	line-height: 25px;
}

.user,#captcha {
	width:323px;
	height: 42px;
	padding: 0px 0px;
	position: relative;
	border: 1px solid;
	border-color: #bfbfbf #d7d7d7 #d7d7d7 #bfbfbf;
	-webkit-border-radius: 4px;
	-moz-border-radius: 4px;
	border-radius: 4px;
	margin-top:16px;
}

.user:hover,#captcha:hover {
	-webkit-transition: box-shadow linear 0.2s;
	-moz-transition: box-shadow linear 0.2s;
	-ms-transition: box-shadow linear 0.2s;
	-o-transition: box-shadow linear 0.2s;
	transition: box-shadow linear 0.2s;
	-webkit-box-shadow: inset 0 1px 2px rgba(0, 0, 0, 0.1), 0 0 8px rgba(82, 168, 236, 0.6);
	-moz-box-shadow: inset 0 1px 2px rgba(0, 0, 0, 0.1), 0 0 8px rgba(82, 168, 236, 0.6);
	box-shadow: inset 0 1px 2px rgba(0, 0, 0, 0.1), 0 0 8px rgba(82, 168, 236, 0.6);
	border: 1px solid #74b9ef;
}

.user input {
	width:285px;
	height: 40px;
	line-height: 40px;
	margin: 0px;
	left: 4px;
	position: absolute;
	left: 4px;
	outline: none;
	border: none;
	background: none;
	text-indent:10px;
	
}
.user .u {
	width: 20px;
	height: 20px;
	float: right;
	margin: 4px 0 0 0 ;
	position: relative;
	cursor: pointer;
	outline: none;
	blr: expression(this.hideFocus = true);
	border: none;
	background: url(../images/icon_user.png) no-repeat;
}
	
.user .p {
	width: 20px;
	height: 20px;
	float: right;
	margin: 4px 0 0 0 ;
	position: relative;
	left: -4px;
	cursor: pointer;
	outline: none;
	blr: expression(this.hideFocus = true);
	border: none;
	background: url(../images/icon_key.png) no-repeat;
}

.captchaImage {
	width:70px;
	height:33px;
	position: relative;
	border:0;
	margin: 4px 6px 0 0 ;
	float: right;
	cursor: pointer;
	outline: none;
	cursor: pointer;
	blr: expression(this.hideFocus = true);
}

.logo{
	padding-left:10px;
}
.content_left{
	width:420px;
	height:302px;
}

.content_rigth{
	width:380px;
	background:#ffffff;
}

input.login_button {
	width:323px;
	height: 52px;
	margin-top:10px;
	*line-height: 32px;
	display: inline-block;
	display: -moz-inline-stack;
	padding: 0px 14px;
	margin-top: 30px;
	cursor: pointer;
	*overflow: visible;
	outline: none;
	blr: expression(this.hideFocus = true);
	border: 1px solid #b7c8d9;
	background: #36B0C8 none repeat scroll 0% 0%;
	-webkit-border-radius: 4px;
	-moz-border-radius: 4px;
	border-radius: 4px;
	font-size: 18px;
	color:#FFF;
}

@-moz-document url-prefix() {
	input.button {
		padding: 0px 10px;
	}
}

input.login_button:hover {
	color: #222222;
	-webkit-transition: box-shadow linear 0.2s;
	-moz-transition: box-shadow linear 0.2s;
	-ms-transition: box-shadow linear 0.2s;
	-o-transition: box-shadow linear 0.2s;
	transition: box-shadow linear 0.2s;
	-webkit-box-shadow: inset 0px 1px 1px rgba(0, 0, 0, 0.075), 0px 0px 6px rgba(82, 168, 236, 0.6);
	-moz-box-shadow: inset 0px 1px 1px rgba(0, 0, 0, 0.075), 0px 0px 6px rgba(82, 168, 236, 0.6);
	box-shadow: inset 0px 1px 1px rgba(0, 0, 0, 0.075), 0px 0px 6px rgba(82, 168, 236, 0.6);
	background-position: 0px -30px;
}

input.login_button:active {
	background-position: 0px -60px;
	font-size:16px;
}


div.xxMessage {
	padding: 10px 30px;
	display: none;
	position: fixed;
	_position: absolute;
	top: 25%;
	_top: expression((document.documentElement.scrollTop || document.body.scrollTop) + Math.round(25 * (document.documentElement.offsetHeight || document.body.clientHeight) / 100) + "px");
	left: 50%;
	z-index: 100;
	overflow: hidden;
	-webkit-box-shadow: 1px 1px 6px #999999;
	-moz-box-shadow: 1px 1px 6px #999999;
	box-shadow: 1px 1px 6px #999999;
	-ms-filter: "progid:DXImageTransform.Microsoft.Shadow(color=#e5e5e5,direction=0,strength=3) progid:DXImageTransform.Microsoft.Shadow(color=#d4d4d4,direction=90,strength=3) progid:DXImageTransform.Microsoft.Shadow(color=#d4d4d4,direction=180,strength=3) progid:DXImageTransform.Microsoft.Shadow(color=#e5e5e5,direction=270,strength=3)";
	filter: progid:DXImageTransform.Microsoft.Shadow(color=#e5e5e5,direction=0,strength=3) progid:DXImageTransform.Microsoft.Shadow(color=#d4d4d4,direction=90,strength=3) progid:DXImageTransform.Microsoft.Shadow(color=#d4d4d4,direction=180,strength=3) progid:DXImageTransform.Microsoft.Shadow(color=#e5e5e5,direction=270,strength=3);
    border: 1px solid #999999;
	background-color: #ffffff;
}

div.xxMessage .messageContent {
	line-height: 24px;
	padding: 0px 10px 0px 30px;
	color: #666666;
}

div.xxMessage .messagewarnIcon {
	background: url(../images/common.gif) -60px -360px no-repeat;
}

div.xxMessage .messageIframe {
	width: 2000px;
	height: 2000px;
	position: absolute;
	left: -100px;
	top: -100px;
	z-index: -1;
}

span.hidden{
	display:none;
}

.verify{
	position: relative;
}